class pig : public Farm_Animal
{
public:
    pig(string name,string feature,string sound,vector<string>resource): Farm_Animal(name,feature,sound, resource){};
    void get_action(string object)
    {
        cout<<name<<' '<< feature<<' '<< object<<endl;
    }
};
class cock : public Farm_Animal
{
public:
    cock(string name,string feature,string sound,vector<string>resource): Farm_Animal(name,feature,sound, resource){

    };
    void get_action(string object)
    {
        cout<<name<<' '<< feature<<' '<< object<<endl;
    }
};
